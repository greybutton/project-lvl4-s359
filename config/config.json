{
  "development": {
    "storage": "./db.development.sqlite",
    "dialect": "sqlite"
  },
  "test": {
    "storage": ":memory:",
    "dialect": "sqlite"
  },
  "production": {
    "dialect": "postgres",
    "protocol": "postgres",
    "use_env_variable": "DATABASE_URL",
    "dialectOptions": {
      "ssl": true
    }
  }
}
